{
  "name": "foamycastle/doctrine-config",
  "description": "a generic doctrine configuration wrapper",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Aaron Sollman",
      "email": "unclepong@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "foamycastle/config": "@dev",
    "doctrine/dbal": "4.4.*",
    "doctrine/orm": "3.5.*",
    "symfony/cache": "^7",
    "foamycastle/helper": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^12"
  },
  "autoload": {
    "psr-4": {
      "Foamycastle\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Foamycastle\\Config\\Tests\\": "test"
    }
  }
}